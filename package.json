{
    "name": "@cloudrex/forge",
    "version": "1.2.5-beta.6",
    "description": "Advanced Discord bot framework for serious bot development",
    "main": "./dist/index.js",
    "keywords": [
        "discord",
        "discord-bot",
        "discord-library",
        "discord-framework"
    ],
    "presets": [
        "env",
        "es2015"
    ],
