{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "one-line": false,
        "trailing-comma": false,
        "radix": false,
        "array-type": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "ordered-imports": false,
        "max-line-length": false,
        "no-console": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
